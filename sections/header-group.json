{"name":"t:sections.header.name","type":"header","sections":{"announcement-bar":{"type":"announcement-bar","blocks":{"announcement_4TaTg8":{"type":"announcement","settings":{"text":"SHOP OUR PACKS OF 3","link":"shopify:\/\/collections\/packs"}}},"block_order":["announcement_4TaTg8"],"custom_css":[],"settings":{"color_scheme":"scheme-1","show_line_separator":true,"show_social":true,"auto_rotate":false,"change_slides_speed":5,"enable_country_selector":false,"enable_language_selector":false}},"header":{"type":"header","custom_css":["a {font-size: 15px; text-align: center;}",".list-menu__item {font-size: 20px; text-align: center;}"],"settings":{"logo_position":"top-center","menu":"main-menu","menu_type_desktop":"mega","sticky_header_type":"none","show_line_separator":true,"color_scheme":"scheme-1","menu_color_scheme":"scheme-1","enable_country_selector":true,"enable_language_selector":true,"enable_customer_avatar":true,"mobile_logo_position":"center","margin_bottom":0,"padding_top":20,"padding_bottom":8}},"custom_liquid_TyMBGa":{"type":"custom-liquid","custom_css":[".description-drawer ul {padding-left: 12px; margin-bottom: 1rem; font-size: 14px; color: #000;}",".description-drawer p {font-size: 14px;}",".description-drawer h3 {font-size: 15px;}"],"name":"t:sections.custom-liquid.presets.name","settings":{"custom_liquid":"<!-- header.liquid or theme.liquid (just before the closing <\/body> tag) -->\n\n<!-- Drawer Overlay (behind the drawer) -->\n<div class=\"drawer-overlay\" onclick=\"closeDescriptionDrawer()\"><\/div>\n\n<!-- The Drawer -->\n<div id=\"product-description-drawer\" class=\"description-drawer\">\n  <div class=\"drawer-content\">\n    <!-- Close Button -->\n    <button class=\"drawer-close-btn\" onclick=\"closeDescriptionDrawer()\">×<\/button>\n\n    <!-- Title & Description will be inserted dynamically -->\n    <h2 class=\"drawer-title\" id=\"drawer-title\">Description<\/h2>\n    <div class=\"drawer-description\" id=\"drawer-description\"><\/div>\n  <\/div>\n<\/div>\n\n<!-- Drawer JavaScript -->\n<script>\n  function openDescriptionDrawer() {\n    \/\/ Grab the button that was clicked\n    const button = event.currentTarget;\n\n    \/\/ Get the drawer and overlay elements\n    const drawer = document.getElementById(\"product-description-drawer\");\n    const overlay = document.querySelector(\".drawer-overlay\");\n\n    \/\/ Pull product data from the button's data attributes\n    const productDescription = button.getAttribute(\"data-product-description\") || \"\";\n\n    \/\/ For description, we can set innerHTML if you trust the content (e.g. HTML formatting)\n    document.getElementById(\"drawer-description\").innerHTML = productDescription;\n\n    \/\/ Activate the drawer and overlay\n    drawer.classList.add(\"active\");\n    overlay.classList.add(\"active\");\n   \n  }\n\n  function closeDescriptionDrawer() {\n    const drawer = document.getElementById(\"product-description-drawer\");\n    const overlay = document.querySelector(\".drawer-overlay\");\n\n    \/\/ Deactivate the drawer and overlay\n    drawer.classList.remove(\"active\");\n    overlay.classList.remove(\"active\");\n\n    \/\/ Restore background scroll\n    document.body.style.overflow = \"\";\n  }\n\n  \/\/ Close on ESC key\n  document.addEventListener(\"keydown\", function(event) {\n    if (event.key === \"Escape\") {\n      closeDescriptionDrawer();\n    }\n  });\n<\/script>\n\n<!-- Drawer CSS -->\n<style>\n  \/***********************************\n    OVERLAY STYLES\n  ************************************\/\n.drawer-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  \/* Use 100vw \/ 100vh or 100% if you prefer *\/\n  width: 100vw;\n  height: 100vh;\n\n  \/* Dark semi-transparent background *\/\n  background: rgba(0, 0, 0, 0.6);\n\n  \/* Start hidden *\/\n  opacity: 0;\n  visibility: hidden;\n\n  \/* Smooth fade-in\/out *\/\n  transition: opacity 0.3s ease, visibility 0.3s ease;\n\n  \/* Must be behind the drawer but above the page content *\/\n  z-index: 3000;\n}\n\n\/* When active, it becomes visible *\/\n.drawer-overlay.active {\n  opacity: 1;\n  visibility: visible;\n}\n\n  \/***********************************\n    DRAWER STYLES\n  ************************************\/\n  .description-drawer {\n    position: fixed;\n    top: 0;\n    right: 0;\n    height: 100%;\n    width: 600px;          \/* Drawer width *\/\n    max-width: 90%;        \/* Ensure it fits on smaller screens *\/\n    background: #fff;\n    transform: translateX(100%); \/* Hidden off-screen by default *\/\n    transition: transform 0.3s ease;\n    z-index: 3001;         \/* Above the overlay *\/\n    display: flex;\n    flex-direction: column;\n  }\n  .description-drawer.active {\n    transform: translateX(0);\n        box-shadow: -5px 0 10px rgba(0, 0, 0, 0.2);\n    \/* Slide into view *\/\n  }\n\n  .drawer-content {\n    padding: 20px;\n    overflow-y: auto;\n    flex-grow: 1;\n    position: relative;\n  }\n\n  .drawer-close-btn {\n    font-size: 24px;\n    background: none;\n    border: none;\n    cursor: pointer;\n    position: absolute;\n    top: 15px;\n    right: 15px;\n  }\n\n  .drawer-title {\n    font-size: 20px;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin-top: 0;\n    margin-bottom: 2rem;\n    padding-top: 10px; \/* spacing so text doesn't clash with the close button *\/\n  }\nh4{\n     font-size: 0x;\n     color: #000;\n}\n\n  .drawer-description{\n    font-size: 10px;\n    color: #333;\n    line-height: 1.5;\n  }\nh3 {\n    font-size: 12px;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin: 3rem 0 0.5rem;\n  }\n  \/***********************************\n    RESPONSIVE (Optional)\n  ************************************\/\n  @media (max-width: 768px) {\n    .description-drawer {\n      width: 100%;\n    }\n  }\n<\/style>","color_scheme":"","padding_top":0,"padding_bottom":0}},"custom_liquid_AFqjzn":{"type":"custom-liquid","name":"t:sections.custom-liquid.presets.name","settings":{"custom_liquid":"<!-- Delivery & Returns Drawer Overlay -->\n<div class=\"drawer-overlay delivery-returns-overlay\" onclick=\"closeDeliveryReturnsDrawer()\"><\/div>\n\n<!-- Delivery & Returns Drawer -->\n<div id=\"delivery-returns-drawer\" class=\"delivery-returns-drawer\">\n  <div class=\"drawer-content\">\n    <!-- Close Button -->\n    <button class=\"drawer-close-btn\" onclick=\"closeDeliveryReturnsDrawer()\">×<\/button>\n    \n    <!-- Title -->\n    <h2 class=\"drawer-title\">Delivery &amp; Returns<\/h2>\n    \n    <!-- Drawer Description (static content) -->\n    <div class=\"drawer-description\" id=\"delivery-returns-content\">\n      <h3>STANDARD DELIVERY - 80 EGP<\/h3>\n      <ul>\n<li><span>Estimated delivery to Cairo and Alexandria within 2-3 business days.<\/span><\/li>\n<li><span>Estimated delivery to Delta, Upper Egypt and Sinai within 3-5 business days.<\/span><\/li>\n<li><span>Free Standard Delivery for orders over 2000 EGP<\/span><\/li>\n      <\/ul>\n\n      <h3>RETURNS AND EXCHANGES<\/h3>\n      <p>\n        We get it - sometimes something just doesn't work for you and you want to return or exchange. \n        As long as your item(s) are still in their original condition, you have up to 30 days to return your item back to us! There will be a 80 EGP shipping fee for the return\/exchange.\n      <\/p>\n\n      <h3>WHAT ITEMS CAN I RETURN?<\/h3>\n      <p>\n        Any unwanted product must be returned within 30 days from when your xced order was delivered. \n        Please see below for any exceptions to this rule:\n      <\/p>\n      <ul>\n        <li><span>All Underwear &amp; Swimwear can’t be returned for hygiene reasons.<\/span><\/li>\n        <li><span>Socks can be returned if they are sealed in their original packaging.<\/span><\/li>\n        <li><span>All bottles are non-returnable due to hygiene reasons.<\/span><\/li>\n      <\/ul>\n    <\/div>\n  <\/div>\n<\/div>\n\n<!-- Delivery & Returns Drawer JavaScript -->\n<script>\n  function openDeliveryReturnsDrawer() {\n    const drawer = document.getElementById(\"delivery-returns-drawer\");\n    const overlay = document.querySelector(\".delivery-returns-overlay\");\n    drawer.classList.add(\"active\");\n    overlay.classList.add(\"active\");\n    document.body.style.overflow = \"\"; \/\/ Prevent background scrolling\n  }\n\n  function closeDeliveryReturnsDrawer() {\n    const drawer = document.getElementById(\"delivery-returns-drawer\");\n    const overlay = document.querySelector(\".delivery-returns-overlay\");\n    drawer.classList.remove(\"active\");\n    overlay.classList.remove(\"active\");\n    document.body.style.overflow = \"\";\n  }\n\n  \/\/ Close on ESC key\n  document.addEventListener(\"keydown\", function(event) {\n    if (event.key === \"Escape\") {\n      closeDeliveryReturnsDrawer();\n    }\n  });\n<\/script>\n\n<!-- Delivery & Returns Drawer CSS -->\n<style>\n  \/* Overlay *\/\n  .delivery-returns-overlay {\n    position: fixed;\n    top: 0; left: 0;\n    width: 100vw; height: 100vh;\n    background: rgba(0,0,0,0.6);\n    opacity: 0;\n    visibility: hidden;\n    transition: opacity 0.3s ease, visibility 0.3s ease;\n    z-index: 3000;\n  }\n  .delivery-returns-overlay.active {\n    opacity: 1;\n    visibility: visible;\n  }\n\n  \/* Drawer Container *\/\n  .delivery-returns-drawer {\n    position: fixed;\n    top: 0; right: 0;\n    width: 600px;           \/* Adjust as needed *\/\n    max-width: 90%;\n    height: 100%;\n    background: #fff;\n    transform: translateX(100%); \/* Hidden off-screen by default *\/\n    transition: transform 0.3s ease;\n    z-index: 3001;\n    display: flex;\n    flex-direction: column;\n  }\n  .delivery-returns-drawer.active {\n    transform: translateX(0);    \/* Slide into view *\/\n    box-shadow: -5px 0 10px rgba(0,0,0,0.2);\n  }\n\n  \/* Drawer Content *\/\n  .delivery-returns-drawer .drawer-content {\n    padding: 20px;\n    overflow-y: auto;\n    flex-grow: 1;\n    position: relative;\n  }\n\n  \/* Close Button *\/\n  .delivery-returns-drawer .drawer-close-btn {\n    font-size: 24px;\n    background: none;\n    border: none;\n    cursor: pointer;\n    position: absolute;\n    top: 15px; right: 15px;\n  }\n\n  \/* Drawer Title *\/\n  .delivery-returns-drawer .drawer-title {\n    font-size: 18px;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin-top: 15px;\n    margin-bottom: 1.5rem;\n    padding-top: 10px;\n  }\n\n  \/* Drawer Description & Headings *\/\n  .delivery-returns-drawer .drawer-description {\n    font-size: 14px;\n    color: #333;\n    line-height: 1.5;\n  }\n  .delivery-returns-drawer .drawer-description h3 {\n    font-size: 12px;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin: 3rem 0 0.5rem;\n  }\n  .delivery-returns-drawer .drawer-description ul {\n    padding-left: 10px;\n    margin-bottom: 1rem;\n    font-size: 10px;\n    color: #000;\n  }\n  .delivery-returns-drawer .drawer-description p {\n    margin-bottom: 1rem;\n    font-size: 10px;\n    color: #000;\n  }\n<\/style>","color_scheme":"","padding_top":0,"padding_bottom":0}},"custom_liquid_3VGXJY":{"type":"custom-liquid","name":"t:sections.custom-liquid.presets.name","settings":{"custom_liquid":"<!-- SIZE CHART OVERLAY -->\n<div class=\"drawer-overlay size-chart-overlay\" onclick=\"closeSizeChartDrawer()\"><\/div>\n\n<!-- SIZE CHART DRAWER -->\n<div id=\"product-size-chart-drawer\" class=\"description-drawer size-chart-drawer\">\n  <div class=\"drawer-content\">\n    <!-- Close Button -->\n    <button class=\"drawer-close-btn\" onclick=\"closeSizeChartDrawer()\">×<\/button>\n\n    <!-- Static Title: \"SIZE CHART\" -->\n    <h2 class=\"drawer-title\" style=\"text-align: centre;\">SIZE GUIDE<\/h2>\n\n    <!-- We'll insert the image dynamically via JS -->\n    <div class=\"drawer-description\" id=\"drawer-size-chart\"><\/div>\n  <\/div>\n<\/div>\n\n<script>\n  function openSizeChartDrawer() {\n    \/\/ Identify the button that was clicked\n    const button = event.currentTarget;\n    \n    \/\/ Grab the new size-chart drawer & overlay\n    const drawer = document.getElementById(\"product-size-chart-drawer\");\n    const overlay = document.querySelector(\".size-chart-overlay\");\n    \n    \/\/ Get the image URL from data attribute\n    const sizeChartUrl = button.getAttribute(\"data-size-chart\") || \"\";\n\n    \/\/ Insert the image into the drawer\n    \/\/ Feel free to style or wrap it in a container if you like\n    document.getElementById(\"drawer-size-chart\").innerHTML = `\n      <img src=\"${sizeChartUrl}\" alt=\"Size Chart\" style=\"max-width: 100%;\" \/>\n    `;\n\n    \/\/ Activate the drawer and overlay\n    drawer.classList.add(\"active\");\n    overlay.classList.add(\"active\");\n\n    \/\/ Prevent background scroll if desired\n    document.body.style.overflow = \"hidden\";\n  }\n\n  function closeSizeChartDrawer() {\n    const drawer = document.getElementById(\"product-size-chart-drawer\");\n    const overlay = document.querySelector(\".size-chart-overlay\");\n\n    \/\/ Deactivate the drawer and overlay\n    drawer.classList.remove(\"active\");\n    overlay.classList.remove(\"active\");\n\n    \/\/ Restore scrolling\n    document.body.style.overflow = \"\";\n  }\n\n  \/\/ Close on ESC\n  document.addEventListener(\"keydown\", function(event) {\n    if (event.key === \"Escape\") {\n      closeSizeChartDrawer();\n    }\n  });\n<\/script>\n\n<style>\n\/* SIZE CHART OVERLAY *\/\n.size-chart-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100vw;\n  height: 100vh;\n  background: rgba(0, 0, 0, 0.6);\n  opacity: 0;\n  visibility: hidden;\n  transition: opacity 0.3s ease, visibility 0.3s ease;\n  z-index: 3000;\n}\n.size-chart-overlay.active {\n  opacity: 1;\n  visibility: visible;\n}\n\/* SIZE CHART DRAWER *\/\n.size-chart-drawer {\n  position: fixed;\n  top: 0;\n  right: 0;\n  height: 100%;\n  width: 600px;       \/* or your preferred width *\/\n  max-width: 90%;\n  background: #fff;\n  transform: translateX(100%);\n  transition: transform 0.3s ease;\n  z-index: 3001;\n  display: flex;\n  flex-direction: column;\n}\n.size-chart-drawer.active {\n  transform: translateX(0);\n  box-shadow: -5px 0 10px rgba(0, 0, 0, 0.2);\n}\n\n.size-chart-drawer .drawer-content {\n  padding: 20px;\n  overflow-y: auto;\n  flex-grow: 1;\n  position: relative;\n}\n\n.size-chart-drawer .drawer-close-btn {\n  font-size: 24px;\n  background: none;\n  border: none;\n  cursor: pointer;\n  position: absolute;\n  top: 15px;\n  right: 15px;\n}\n\n.size-chart-drawer .drawer-title {\n  font-size: 20px;\n  font-weight: bold;\n  text-transform: uppercase;\n  margin-top: 0;\n  margin-bottom: 2rem;\n  padding-top: 10px;\n}\n\n.size-chart-drawer .drawer-description {\n  font-size: 14px;\n  color: #333;\n  line-height: 1.5;\n}\n\n\/* Responsive (optional) *\/\n@media (max-width: 768px) {\n  .size-chart-drawer {\n    width: 100%;\n  }\n}\n\n<\/style>","color_scheme":"","padding_top":0,"padding_bottom":0}}},"order":["announcement-bar","header","custom_liquid_TyMBGa","custom_liquid_AFqjzn","custom_liquid_3VGXJY"]}